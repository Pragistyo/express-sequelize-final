<div class="container">
<div class="row">
<div class="col-md-8 ml-md-auto mr-md-auto" style="margin-top:50px">
<h1 class="text-center" style="margin:0">EDIT Suppliers</h1>
  <body>
    <form action="/suppliers/edit/<%=data.id%>" method="post">
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="inputName" class="col-form-label">Suppliers Name</label>
          <input type="text" name="name" class="form-control"value="<%=data.name%>">
        </div>
        <div class="form-group col-md-6">
          <label for="inputName" class="col-form-label">Suppliers City</label>
          <input type="text" name="kota" class="form-control"value="<%=data.kota%>">
        </div>
      </div>
        <div class="form-group">
          <label for="inputAlamat" class="col-form-label">Detail Address:</label>
          <textarea class="form-control" id="inputAlamat" name="alamat" rows="3"></textarea>
        </div>
      <button type="submit" name="button" class="btn btn-primary">EDIT</button>
      <a class="btn btn-danger"href="/suppliers">Cancel</a>
    </form>
  </body>
</div>
  </div>
  </div>
