<%- include partial/header%>
<%- include partial/navbar%>
  </head>
  <body>
    <div class="container">
        <div class="row">
          <div class="col-md-4 ml-md-auto mr-md-auto" style="margin-top:50px">
            <h1 style="margin:0;">Search Page</h1>
            <form class="" action="" method="get">
              <div class="form-row">
                <div class="form-group col-md-12">
                  <label for="inputName" class="col-form-label">Name Item</label>
                  <input type="text" name="name" class="form-control" id="inputName" placeholder="  ">
                </div>
                <div class="form-group col-md-6">
                  <label for="inputBrand" class="col-form-label">Min Price</label>
                  <input type="text" name="min" class="form-control" id="inputBrand" placeholder="">
                </div>
                <div class="form-group col-md-6">
                  <label for="inputCode" class="col-form-label">Max Price</label>
                  <input aria-describedby="codeHelp" type="text" name="max" class="form-control" id="inputCode" placeholder="">
                </div>
              </div>
              <button type="submit" class="btn btn-primary">Search</button>
            </form>
          </div>
        </div>
      </div>
  </body>
</html>
